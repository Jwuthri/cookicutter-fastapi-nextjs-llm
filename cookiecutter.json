{
    "project_name": "My AI Agent App",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-').replace('_', '-') }}",
    "description": "A FastAPI + Next.js AI agent application with OpenRouter and Agno",
    "author_name": "Your Name",
    "author_email": "your.email@example.com",
    "version": "0.1.0",
    "python_version": "3.11",
    "node_version": "18",
    "use_websockets": ["no", "yes"],
    "include_database": ["postgresql", "sqlite", "none"],
    "llm_provider": "openrouter",
    "default_model": ["gpt-4.1", "claude-4-sonnet", "gemini-2.5-pro", "custom"],
    "vector_database": ["pinecone", "weaviate", "qdrant", "chromadb", "none"],
    "use_agno_agents": ["yes", "no"],
    "agent_type": ["single", "multi-agent", "workflow"],
    "memory_type": ["vector", "redis", "hybrid", "in-memory"],
    "frontend_port": "3000",
    "backend_port": "8000",
    "postgres_port": "5432",
    "_copy_without_render": [
        "*.js",
        "*.jsx", 
        "*.ts",
        "*.tsx",
        "*.json",
        "*.css",
        "*.scss",
        "*.sass",
        "*.less",
        "*.svg",
        "*.png",
        "*.jpg",
        "*.jpeg",
        "*.gif",
        "*.ico",
        "*.woff",
        "*.woff2",
        "*.ttf",
        "*.eot"
    ]
}
