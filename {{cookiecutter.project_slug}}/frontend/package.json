{
  "name": "{{cookiecutter.project_slug}}-frontend",
  "version": "{{cookiecutter.version}}",
  "description": "{{cookiecutter.description}} - Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev -p {{cookiecutter.frontend_port}}",
    "build": "next build",
    "start": "next start -p {{cookiecutter.frontend_port}}",
    "lint": "next lint",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "axios": "1.6.2",
    "lucide-react": "0.294.0",
    "date-fns": "2.30.0",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0",
    "clsx": "2.0.0",
    "tailwind-merge": "2.2.0",
    "@clerk/nextjs": "^4.29.3",
    "framer-motion": "^10.16.16",
    "react-hot-toast": "^2.4.1"{% if cookiecutter.use_websockets == "yes" %},
    "socket.io-client": "4.7.4"{% endif %}
  },
  "devDependencies": {
    "@types/node": "20.9.2",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "postcss": "8.4.32",
    "tailwindcss": "3.3.6",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
